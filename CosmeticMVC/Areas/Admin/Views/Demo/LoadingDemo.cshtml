
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>LoadingDemo</title>
    <script src="~/Scripts/jqChart/jquery-1.11.1.min.js"></script>
    <script lang="javascript" type="text/javascript">
        $(document).ready(function () {
            var today = new Date();
            var dd = today.getDate();
            var mm = today.getMonth()+1; //January is 0!
            var yyyy = today.getFullYear();

            if(dd<10) {
                dd='0'+dd
            } 

            if(mm<10) {
                mm='0'+mm
            } 
            today = mm + '/' + dd + '/' + yyyy;
            console.log(today);
            $.post('/Admin/Demo/GetDateTime', { dt: today }, function () {

            })
        });
    </script>
</head>
<body>
    <div>
        
    </div>
</body>
</html>
