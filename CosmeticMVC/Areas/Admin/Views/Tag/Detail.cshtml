@model List<DataLayer.ViewModel.TagDetailViewModel>
@{
    ViewBag.Title = "Detail";
    Layout = "~/Areas/Admin/Views/Shared/BackendLayout.cshtml";
}


    <h2>Bài viết liên quan theo từ khóa : <strong>@ViewBag.TagName</strong></h2>

<div class="row">
    <div class="col-md-12">
        <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover">
                <thead>
                    <tr>
                        <th>Tên bài viết</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td><a href="/Admin/Content/Edit/@item.ContentID">@item.ContentName</a></td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>